<?xml version='1.0' encoding='utf-8'?>
<plugin 
    id="IOSSecuritySuite" 
    version="0.0.1" 
    xmlns="http://apache.org/cordova/ns/plugins/1.0" 
    xmlns:android="http://schemas.android.com/apk/res/android"
>
    <name>IOSSecuritySuite</name>
    <js-module name="IOSSecuritySuite" src="www/IOSSecuritySuite.js">
        <clobbers target="IOSSecuritySuite" />
    </js-module>
    <platform name="ios">
    <config-file target="config.xml" parent="/*">
        <feature name="IOSSecuritySuite">
        <param name="ios-package" value="IOSSecuritySuite" />
        </feature>
    </config-file>
    <header-file src="src/IOSSecuritySuite.h" />
    <source-file src="src/IOSSecuritySuite.swift" />
    <source-file src="src/DebuggerChecker.swift" />
    <source-file src="src/EmulatorChecker.swift" />
    <source-file src="src/JailbreakChecker.swift" />
    <source-file src="src/ReverseEngineeringToolsChecker.swift" />
    </platform>
</plugin>
